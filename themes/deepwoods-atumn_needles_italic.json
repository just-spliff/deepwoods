{
  "name": "Deepwoods Autumn Needles Italic",
  "semanticHighlighting": true,
  "colors": {
    // Editor
    "editor.background": "#090806",
    "editor.foreground": "#FFF8F0",
    "editor.selectionBackground": "#E9A86C33",
    "editor.selectionHighlightBackground": "#E9A86C26",
    "editorLineNumber.foreground": "#5C5248",
    "editorGroupHeader.tabsBackground": "#090806",
    "editorWidget.background": "#090806",
    "editorWarning.foreground": "#E9A86C",
    "editorError.foreground": "#E36B5F",
    "editorOverviewRuler.border": "#090806",
    "editorGutter.addedBackground": "#8AA37C",
    "editorGutter.deletedBackground": "#E36B5F",
    "editorGutter.modifiedBackground": "#E9A86C",
    "diffEditor.insertedTextBackground": "#8AA37C15",
    "diffEditor.insertedLineBackground": "#8AA37C15",
    "diffEditor.removedTextBackground": "#E36B5F15",
    "diffEditor.removedLineBackground": "#E36B5F15",
    "editorInlayHint.foreground": "#A49B8F",
    "editorInlayHint.background": "#14100D",

    // Terminal
    "terminal.background": "#090806",
    "terminal.foreground": "#FFF8F0",
    "terminalCursor.foreground": "#F4E3C3",
    "terminalCursor.background": "#090806",
    "terminal.selectionBackground": "#3A271880",

    "terminal.ansiBlack": "#15100C",
    "terminal.ansiBrightBlack": "#43372D",

    "terminal.ansiRed": "#E36B5F",
    "terminal.ansiBrightRed": "#F18977",

    "terminal.ansiGreen": "#8AA37C",
    "terminal.ansiBrightGreen": "#B6C79B",

    "terminal.ansiYellow": "#E9A86C",
    "terminal.ansiBrightYellow": "#F4C98D",

    "terminal.ansiBlue": "#A49B8F",
    "terminal.ansiBrightBlue": "#C0B7AA",

    "terminal.ansiMagenta": "#BF8A82",
    "terminal.ansiBrightMagenta": "#D9A496",

    "terminal.ansiCyan": "#C2D2B7",
    "terminal.ansiBrightCyan": "#E4F0DE",

    "terminal.ansiWhite": "#FFF8F0",
    "terminal.ansiBrightWhite": "#FFFFFF",

    // Sidebar
    "sideBar.background": "#090806",
    "sideBarTitle.foreground": "#B8ADA0",
    "sideBarSectionHeader.foreground": "#B8ADA0",
    "sideBarSectionHeader.background": "#090806",

    // Activity bar
    "activityBar.background": "#090806",
    "activityBar.foreground": "#B8ADA0",
    "activityBarBadge.background": "#E9A86C",
    "activityBarBadge.foreground": "#130D08",

    // Title bar
    "titleBar.activeBackground": "#090806",
    "titleBar.inactiveBackground": "#090806",
    "titleBar.activeForeground": "#A3988B",
    "titleBar.inactiveForeground": "#7A7065",

    // Tab
    "tab.border": "#090806",
    "tab.activeBackground": "#110E0A",
    "tab.activeBorder": "#E9A86C",
    "tab.inactiveBackground": "#090806",

    // Status bar
    "statusBar.debuggingForeground": "#090806",
    "statusBar.debuggingBackground": "#E9A86C",
    "statusBar.background": "#090806",
    "statusBar.foreground": "#B8ADA0",
    "statusBarItem.remoteBackground": "#E9A86C",
    "statusBarItem.remoteForeground": "#130D08",

    // Lists
    "list.activeSelectionForeground": "#F4E3C3",
    "list.inactiveSelectionBackground": "#18100C",
    "list.activeSelectionBackground": "#18100C",
    "list.hoverBackground": "#1E140F",
    "list.errorForeground": "#E36B5F",
    "list.highlightForeground": "#E9A86C",

    // Global
    "badge.background": "#E9A86C",
    "badge.foreground": "#130D08",
    "button.background": "#E9A86C",
    "button.hoverBackground": "#F0B880",
    "button.foreground": "#130D08",
    "focusBorder": "#E9A86C",
    "icon.foreground": "#B8ADA0",
    "input.background": "#140F0B",
    "selection.background": "#6B5A4B",

    // Brackets
    "editorBracketHighlight.foreground1": "#B8ADA0",
    "editorBracketHighlight.foreground2": "#B8ADA0",
    "editorBracketHighlight.foreground3": "#B8ADA0",
    "editorBracketHighlight.foreground4": "#B8ADA0",
    "editorBracketHighlight.foreground5": "#B8ADA0",
    "editorBracketHighlight.foreground6": "#B8ADA0",
    "editorBracketHighlight.unexpectedBracket.foreground": "#E36B5F",

    // Links
    "textLink.foreground": "#E9A86C",
    "textLink.activeForeground": "#F4C98D",

    // Tooltips
    "editorHoverWidget.background": "#15100C",
    "editorHoverWidget.border": "#241B14",

    // Scrollbar
    "scrollbarSlider.background": "#3E312580",
    "scrollbarSlider.hoverBackground": "#3E3125",

    // Settings
    "settings.modifiedItemIndicator": "#E9A86C"
