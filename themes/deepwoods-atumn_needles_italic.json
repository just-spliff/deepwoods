  "semanticTokenColors": {
    "parameter": {
      "foreground": "#F2C27E",
      "fontStyle": "italic"
    },
    "variable.readonly": {
      "foreground": "#F4E3C3",
      "fontStyle": "italic"
    },
    "property": {
      "foreground": "#F7EADB",
      "fontStyle": "italic"
    },
    "namespace": {
      "foreground": "#8AA37C",
      "fontStyle": "italic"
    },
    "type": {
      "foreground": "#C0B7AA",
      "fontStyle": "italic"
    },
    "enumMember": {
      "foreground": "#F2C27E",
      "fontStyle": "italic"
    },
    "function": {
      "foreground": "#F0B880",
      "fontStyle": "italic"
    },
    "method": "#F0B880",
    "event": "#8AA37C"
  }
}
